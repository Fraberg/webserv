server {
    listen 	8080
    error	./www/old/error
	name	server1
	host	localhost
	
    location / 		{
        root	./www/old
		index	index.html
		method	GET POST HEAD OPTIONS PUT
		cgi .php
		#cgi .cgi
		php_path /usr/local/php5-7.3.8-20190811-205217/bin/php-cgi
		#cgi_path ./www/CGI/
    }
    location /test	{
        root    ./www/old/test
		index	index.html
		auto_index	0
		method	GET POST HEAD OPTIONS PUT
    }
    location /auto_index	{
        root    ./www/old/vide
		index	videvidevide
		auto_index	1
		method	GET POST HEAD OPTIONS PUT
    }
}
